<h2>Latest posts:</h2>

<table class="table table-striped">
	<thead>
		<tr>
			<th>id</th>
			<th>content</th>
			<th>date-time</th>
		</tr>			
	</thead>
	<tbody>
		<% @result.each do |post| %>
			<tr>
				<td><%=post['id'] %></td>
				<td><%=post['content'] %></td>
				<td><%=post['created_date'] %></td>
			</tr>
		<% end %>

	</tbody>
</table>
